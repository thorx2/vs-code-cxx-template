{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/Fe${workspaceFolder}\\out\\main.exe",
        "/Fd${workspaceFolder}\\obj\\$main.obj",
        "/Fo${workspaceFolder}\\out\\",
        "/I",
        "${workspaceFolder}\\include",
        "${workspaceFolder}\\src\\*.cpp",
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
